@model FormW2SDK.Models.FormW2Create.FormW2CreateRequest
@using FormW2SDK.Models.Utilities
@using static FormW2SDK.Models.Base.EntityBase;
<head>
    <link href="~/css/icons-mdi/css/materialdesignicons.min.css" rel="stylesheet" />
    <!-- Include jQuery -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <!-- Include jQuery UI Datepicker -->
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

</head>
<div class="container">
    <div class="w-75 m-auto mt-4">
        <div class="border-1 border rounded-2 pb-4">
            @if (Model != null && Model.SubmissionManiFest != null)
            {
                <h2 class="head-h1 px-12px">
                    Update Form W2
                </h2>
            }
            else
            {
                <h2 class="head-h1 px-12px">
                    Create Form W2
                </h2>
            }
            <div class="px-4">
                @using (Html.BeginForm("", "", FormMethod.Post, new { id = "W2ReturnForm" }))
                {
                    <div class="fs-20 text-dark">
                        @if (!string.IsNullOrWhiteSpace(Model.ReturnHeader.Business.BusinessNm))
                        {
                            <span class=" position-relative pb-0  mb-3"></span> <span class="head-1 me-3">@Model.ReturnHeader.Business.BusinessNm</span> <span class="text-muted fs-14"></span>
                        }
                        else
                        {

                            <span class=" position-relative pb-0  mb-3"></span> <span class="head-1 me-3">@Model.ReturnHeader.Business.FirstNm</span> <span class="text-muted fs-11"></span>
                            <span class=" position-relative pb-0  mb-3"></span> <span class="head-1 me-3">@Model.ReturnHeader.Business.MiddleNm</span> <span class="text-muted fs-14"></span>
                            <span class=" position-relative pb-0  mb-3"></span> <span class="head-1 me-3">@Model.ReturnHeader.Business.LastNm</span> <span class="text-muted fs-14"></span>
                            <span class=" position-relative pb-0  mb-3"></span> <span class="head-1 me-3">@Model.ReturnHeader.Business.Suffix</span> <span class="text-muted fs-14"></span>
                        }
                        @if (Model.ReturnHeader.Business.EINorSSN.Length > 10)
                            {
                                <span>(SSN : @Model.ReturnHeader.Business.EINorSSN)</span>
                            }
                            else
                            {
                                <span>(EIN : @Model.ReturnHeader.Business.EINorSSN)</span>
                            }
                    </div>
                    <div class="col-md-6 mt-3">
                        <div class="labelName">
                            <label class="control-label"><span class="text-danger">*</span>Tax Year:</label>
                            @Html.TextBoxFor(model => model.SubmissionManiFest.TaxYear, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="row d-flex justify-content-center mb-15px bg-white">
                        <div class='col mt-3'>
                            <span class="text-danger">*</span>
                            @Html.CheckBoxFor(model => model.SubmissionManiFest.IsFederalFilingNullable,new { @class = "css-checkbox me-1 form-check-input cursor-pointer" })<span class="me-3">IsFederalFiling</span>
                            @Html.CheckBoxFor(model =>  model.SubmissionManiFest.IsStateFilingNullable, new { @class = "css-checkbox me-1 form-check-input cursor-pointer"})<span class="me-3">IsStateFiling</span>
                            @Html.CheckBoxFor(model => model.SubmissionManiFest.IsPostalNullable, new { @class = "css-checkbox me-1 form-check-input cursor-pointer" })<span class="me-3">IsPostal</span>
                            @Html.CheckBoxFor(model => model.SubmissionManiFest.IsOnlineAccessNullable, new { @class = "css-checkbox me-1 form-check-input cursor-pointer" })<span class="me-3">IsOnlineAccess</span>
                        </div>
                    </div>
                    for (var i = 0; i < 1; i++)
                    {
                        <div class="row">
                            <h2 class="sub-head text-left mt-4 mb-3 ">
                                Employee Details
                            </h2>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>SequenceId:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].SequenceId, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>SSN:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.SSN, new { @class = "form-control"})
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>First Name:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.FirstNm, new { @class = "form-control"})
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label">Middle Name:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.MiddleNm, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-2">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>Last Name:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.LastNm, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label">Suffix:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.Suffix, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger"></span>Phone:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.Phone, new { @class = "form-control"})
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger"></span>Email:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.Email, new { @class = "form-control"})
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                </div>
                                <div class="col-md-6">
                                    <span>@Html.CheckBoxFor(model => model.ReturnData[i].Employee.IsForeignNullable, new { onchange = "enableOrDisableFieldsForRecipients()", @class = "css-checkbox form-check-input", id = "RecipientIsForeign" }) Is Foreign?</span>

                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger"></span>Fax:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.Fax, new { @class = "form-control"})
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>Address1:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.USAddress.Address1, new { @id = "RecipientUSAddress1", @class = "form-control" })
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.ForeignAddress.Address1, new { @id = "RecipientForeignAddress1", @style = "display:none", @class = "form-control" })
                                    </div>
                                </div>
                            </div>

                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger"></span>Address2:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.USAddress.Address2, new { @id = "RecipientUSAddress2", @class = "form-control" })
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.ForeignAddress.Address2, new { @id = "RecipientForeignAddress2", @style = "display:none", @class = "form-control" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label"><span class="text-danger">*</span>City:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.USAddress.City, new { @id = "RecipientUsaddressCity", @class = "form-control" })
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.ForeignAddress.City, new { @id = "RecipientForeginAddressCity", @class = "form-control", @style = "display:none" })
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label" id="state_lbl_recipient"><span class="text-danger">*</span>State:</label>
                                        @Html.DropDownListFor(model => model.ReturnData[i].Employee.USAddress.State, Enum.GetValues(typeof(USStates)).Cast
                                <USStates>
                                ().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), "State", new { @id = "RecipientState",@class = "form-control form-select" })
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.ForeignAddress.ProvinceOrStateNm, new { @id = "RecipientProvinceOrStateNm", @class = "form-control", @style = "display:none" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label" id="zip_code_lbl_recipient"><span class="text-danger">*</span>ZipCd:</label>
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.USAddress.ZipCd, new { @id = "RecipientZipCd", @class = "form-control"})
                                        @Html.TextBoxFor(model => model.ReturnData[i].Employee.ForeignAddress.PostalCd, new { @id = "RecipientPostalCd", @class = "form-control", @style = "display:none" })
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-center mb-15px">
                                <div class="col-md-6">
                                    <div class="labelName">
                                        <label class="control-label" id="recipientcountry_lbl" style="display:none;"><span class="text-danger">*</span>Country:</label>
                                        @Html.DropDownListFor(model => model.ReturnData[i].Employee.ForeignAddress.Country,Enum.GetValues(typeof(CountryList)).Cast
                                <CountryList>
                                ().Select(x => new SelectListItem { Text = Utility.GetEnumDisplayName(x), Value = (x).ToString() }), "Country", new { @id = "recipientcountry", @class = "form-control form-select", @style = "display:none" })
                                    </div>
                                </div>
                                <div class="col-md-6">
                                </div>
                            </div>

                        </div>
                        <h2 class="sub-head text-left mt-4 mb-3 ">Form W2 Details</h2>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Wages, tips, other compensation</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B1Wages, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Federal income tax withheld</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B2FedTaxWH, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Social security wages</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B3SocSecWages, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Social security tax withheld</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B4SocSecTaxWH, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Medicare wages and tips</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B5MediWages, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Medicare tax withheld:</label>
                                    <div class="input-group mb-3 ">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B6MediTaxWH, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Social security tips</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B7SocSecTips, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Allocated tips</label>
                                    <div class="input-group mb-3 ">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B8AllocatedTips, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Dependent care benefits</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B10DependtCareBenefits, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                            </div>
                        </div>
                        <h2 class="sub-head text-left  mb-3 ">Nonqualified plans</h2>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Section 457</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B11Sec457Plan, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Non Section 457</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B11NonSec457Plan, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>


                        </div>
                        <h2 class="sub-head text-left  mb-3 ">Box12CodeOptions</h2>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Code</label>
                                    <div class="input-group mb-3 ">

                                        @Html.DropDownListFor(model => model.ReturnData[i].W2FormData.B12aCd, Enum.GetValues(typeof(Box12Codes)).Cast<Box12Codes>().Select(x => new SelectListItem { Value = (x).ToString() ,Text=(x).ToString()}), "Code", new { @class = "form-control form-select" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Amount</label>
                                    <div class="input-group mb-3 ">
                                        <span class="input-group-text">$</span>

                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B12aAmt, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Code</label>
                                    <div class="input-group mb-3 ">
                                        @Html.DropDownListFor(model => model.ReturnData[i].W2FormData.B12bCd, Enum.GetValues(typeof(Box12Codes)).Cast<Box12Codes>().Select(x => new SelectListItem { Value = (x).ToString(),Text=  (x).ToString()}), "Code", new { @class = "form-control form-select" })

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Amount</label>
                                    <div class="input-group mb-3 ">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B12bAmt, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Code</label>
                                    <div class="input-group mb-3 ">
                                        @Html.DropDownListFor(model => model.ReturnData[i].W2FormData.B12cCd, Enum.GetValues(typeof(Box12Codes)).Cast<Box12Codes>().Select(x => new SelectListItem { Value = (x).ToString(),Text=(x).ToString() }), "Code", new { @class = "form-control form-select" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Amount</label>
                                    <div class="input-group mb-3 ">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B12cAmt, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Code</label>
                                    <div class="input-group mb-3 ">
                                        @Html.DropDownListFor(model => model.ReturnData[i].W2FormData.B12dCd, Enum.GetValues(typeof(Box12Codes)).Cast<Box12Codes>().Select(x => new SelectListItem { Value = (x).ToString(),Text=(x).ToString() }), "Code", new { @class = "form-control form-select" })

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Amount</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B12dAmt, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row d-flex justify-content-center mb-15px">
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">Other</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.B14Other, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="labelName">
                                    <label class="control-label">ControlNum</label>
                                    <div class="input-group mb-3">
                                        <span class="input-group-text">$</span>
                                        @Html.TextBoxFor(model => model.ReturnData[i].W2FormData.ControlNum, new { @class = "form-control" })
                                    </div>
                                </div>
                            </div>
                        </div>
                        @Html.CheckBoxFor(model => model.ReturnData[i].W2FormData.B13IsStatEmp,new { @class = "css-checkbox me-1 form-check-input cursor-pointer" })
                        <span class="me-3">Statutory Employee</span>
                        @Html.CheckBoxFor(model =>  model.ReturnData[i].W2FormData.B13IsRetPlan, new { @class = "css-checkbox me-1 form-check-input cursor-pointer" })
                        <span class="me-3">Retirement plan</span>
