@model TinMatchingRecipientsSDK.Models.TinMatchingResponse.TinMatchingGetStatusResponse



<div class="modal-header">
    <h5 class="modal-title" id="exampleModalToggleLabel">Get TIN Matching Status</h5>
    <button type="button" class="btn-close bg-white rounded-circle border-0 fw-bold position-absolute end-n-10 top-n-13 modal-close button-hide fs-18 fw-500" data-bs-dismiss="modal" aria-label="Close">X</button>
</div>
<div class="modal-body">

    @if (Model != null)
    {
        if (Model.Errors != null && Model.Errors.Count > 0)
        {
            <h2 class="tabel-sub-head">Error</h2>
            <div class="table-container mb-3">
                <table>
                    <tbody>
                        
                        @foreach (var item in Model.Errors)
                        {
                            <tr>
                                <td class="fw-bold">Id</td>
                                <td class=" align-top">@item.Id</td>
                            </tr>
                            <tr>
                                <td class="fw-bold align-top">Name</td>
                                <td class="">@item.Name</td>
                            </tr>
                            <tr>
                                <td class="fw-bold">Message</td>
                                <td class=" border-radious-bottom-right align-top">@item.Message</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        }
        if (Model.SubmissionId != Guid.Empty)
        {
            <div class="table-container mb-3">
                <table>
                    <tbody>
                        <tr>
                            <td class="text-left fw-bold">Submission Id</td>
                            <td class="text-left align-top">

                                @if (Model.SubmissionId != Guid.Empty)
                                {
                                    @Model.SubmissionId
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td class="text-left fw-bold">Record Id</td>
                            <td class="text-left align-top">

                                @if (Model.RecordId != Guid.Empty)
                                {
                                    @Model.RecordId
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td class="text-left fw-bold">Sequence Id</td>
                            <td class="text-left align-top">

                                @if (Model.SequenceId != null)
                                {
                                    @Model.SequenceId
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td class="text-left fw-bold">Recipient Id</td>
                            <td class="text-left align-top">

                                @if (Model.RecipientId != null)
                                {
                                    @Model.RecipientId
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td class="text-left fw-bold">TIN Type</td>
                            <td class="text-left border-radious-bottom-right align-top">

                                @if (Model.TINType != null)
                                {
                                    @Model.TINType
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>

                        </tr>
                        <tr>
                            <td class="text-left  fw-bold">TIN</td>
                            <td class="text-left border-radious-bottom-right align-top">

                                @if (Model.TIN != null)
                                {
                                    @Model.TIN
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>

                        </tr>
                        <tr>
                            <td class="text-left  fw-bold">Status</td>
                            <td class="text-left border-radious-bottom-right align-top">

                                @if (Model.Status != null)
                                {
                                    @Model.Status
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td class="text-left  fw-bold">StatusTs</td>
                            <td class="text-left border-radious-bottom-right align-top">

                                @if (Model.StatusTs != null)
                                {
                                    @Model.StatusTs
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>

                        </tr>
                        <tr>
                            <td class="text-left  fw-bold">NumOfAttemptsRmng</td>
                            <td class="text-left border-radious-bottom-right align-top">

                                @if (Model.NumOfAttemptsRmng != null)
                                {
                                    @Model.NumOfAttemptsRmng
                                }
                                else
                                {
                                    <span>-</span>
                                }
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        }
    }
</div>