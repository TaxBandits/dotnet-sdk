<div class="modal-header">
    <h5 class="modal-title" id="exampleModalToggleLabel">NEC RequestPdfUrl Response</h5>
    <button type="button" class="btn-close bg-white rounded-circle border-0 fw-bold position-absolute end-n-10 top-n-13 modal-close button-hide fs-18 fw-500" data-bs-dismiss="modal" aria-label="Close">X</button>
</div>
<div class="modal-body">
    @if (TempData["PdfUrl"] != null)
    {
        <div>
            <iframe src="@Url.Action("RequestPdfUrls", "Form1099Nec", new { PdfUrl = TempData["PdfUrl"]})" style="width:100%;height:600px"></iframe>
        </div>
    }
</div>