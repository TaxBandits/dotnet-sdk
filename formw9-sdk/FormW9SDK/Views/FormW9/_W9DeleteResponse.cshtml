@model FormW9SDK.Models.FormW9.W9CommonDeleteResponse
<div class="modal-header">
    <h5 class="modal-title" id="exampleModalToggleLabel">Form W-9 Delete Response</h5>
    <button type="button" class="btn-close bg-white rounded-circle border-0 fw-bold position-absolute end-n-10 top-n-13 modal-close button-hide fs-18 fw-500" data-bs-dismiss="modal" aria-label="Close">X</button>
</div>
<div class="modal-body">
    @if (Model != null)
    {
        if (Model.Errors != null && Model.Errors.Count > 0)
        {
            <h2 class="tabel-sub-head">Error</h2>
            <div class="table-container mb-3">
                <table>
                    <tbody>
                        <tr class="fw-600 ">
                            <th class="text-center fw-600" width="30%">Id</th>
                            <th class="text-center">Name</th>
                            <th class="text-center">Message</th>
                        </tr>
                        @foreach (var item in Model.Errors)
                        {
                            <tr>
                                <td class="text-center align-top">@item.Id</td>
                                <td class="text-center align-top">@item.Name</td>
                                <td class="text-center border-radious-bottom-right align-top">@item.Message</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        }
        
    }
    
    @if (Model != null && Model.FormW9Records!=null )
    {
        <h2 class="tabel-sub-head">Success Records</h2>
        <div class="table-container mb-3">
            <table>
                <tbody>
                    <tr class="fw-600 ">
                        <th class="text-center">Submission Id</th>
                        <th class="text-center">Email</th>
                        <th class="text-center">StatusTs</th>
                        <th class="text-center">PayeeRef</th>
                        <th class="text-center">W9Status</th>
                    </tr>
                    <tr>
                        @foreach (var item in Model.FormW9Records)
                        {
                            <td class="text-center border-radious-bottom-right align-top">@item.SubmissionId</td>
                            <td class="text-center border-radious-bottom-right align-top">@item.Email</td>
                            <td class="text-center border-radious-bottom-right align-top">@item.StatusTs</td>
                            <td class="text-center border-radious-bottom-right align-top">@item.PayeeRef</td>
                            <td class="text-center border-radious-bottom-right align-top text-success">@item.W9Status</td>
                        }
                    </tr>
                </tbody>
            </table>
        </div>
    }
</div>